{
  "name": "Java Spring Boot Development",
  "image": "mcr.microsoft.com/devcontainers/java:17",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "pivotal.vscode-spring-boot",
        "redhat.java",
        "vscjava.vscode-maven"
      ]
    }
  },
  "forwardPorts": [8080],
  "postCreateCommand": "mvn clean compile"
}